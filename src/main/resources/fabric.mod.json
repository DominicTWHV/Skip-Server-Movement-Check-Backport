{
	"schemaVersion": 1,
	"id": "skipservercheck",
	"version": "${version}",
	"name": "Frostbyte's Skip Server Movement Check",
	"description": "Skips the movement check on the server side that causes rubber-banding due to \"moved too quickly\" or \"moved wrongly\".",
	"authors": [
		"FrostbyteGames"
	],
	"license": "CC-BY-NC",
	"icon": "assets/skipservercheck/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.frostbyte.skipservercheck.SkipServerMovementCheck"
		]
	},
	"mixins": [
		"skipservercheck.mixins.json"
	],
	"accessWidener": "skipservercheck.accesswidener",
	"depends": {
		"fabricloader": ">=0.14.21",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	}
}